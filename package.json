{
  "name": "jsembroidery",
  "version": "0.0.1",
  "description": "Javascript library for the reading and writing of embroidery files.",
  "author": "Leandro Sbrissa",
  "homepage": "https://github.com/leguass7/jsembroidery#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint src --ext js,ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "t": "nodemon ./src/test/index.js",
    "test": "cross-env CI=1 jest",
    "test:update": "cross-env CI=1 jest --coverage --updateSnapshot",
    "py": "python ./src/test/index.py"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leguass7/jsembroidery.git"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "javascript",
    "embroidery",
    "nodejs"
  ],
  "bugs": {
    "url": "https://github.com/leguass7/jsembroidery/issues"
  },
  "peerDependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-jest": "^25.2.4",
    "babel-plugin-import": "^1.13.0",
    "chunk": "^0.0.2",
    "core-js": "3",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.2.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "sucrase": "^3.12.1",
    "typescript": "^3.8.3"
